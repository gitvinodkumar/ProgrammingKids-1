<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">



<bean id="bike" class="com.nt.beans.Bike">

<property name="properties">

<list>

<value>this is very fast</value>
<value>this speed is very high</value>
</list>

</property>
<property name="numbers">

<set>
<value>1234</value>
<value>3456</value>
<value>45768</value>
<value>8900</value>
</set>

</property>



</bean>
<bean id="pulsor" class="com.nt.beans.Pulsor" parent="bike">

<property name="properties">
<list merge="true">
<value>this is belooongs to hari krishna</value>

</list>

</property>
<property name="numbers">

<set merge="true">
<value>500000 </value>
</set>
</property>
<property name="phno">
<set merge="true">
<value>64647646</value>


</set>

</property>


</bean>
</beans>
